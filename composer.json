{
  "name": "brandnewteam/laravel-settings",
  "description": "Simple Settings package for laravel application",
  "type": "library",
  "keywords": ["Laravel", "Settings"],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "brandnewteam\\Settings\\App\\": "src/app"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "laravel/framework": ">=6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "brandnewteam\\Settings\\App\\Providers\\SettingServiceProvider"
      ],
      "aliases": {
        "Settings": "brandnewteam\\Settings\\App\\Facades\\Setting"
      }
    }
  }
}
